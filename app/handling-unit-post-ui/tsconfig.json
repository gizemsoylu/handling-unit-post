{
    "compilerOptions": {
      "target": "es2022",
      "module": "es2022",
      "skipLibCheck": false,
      "allowJs": false,
      "strict": true,
      "sourceMap": true,
      "strictPropertyInitialization": false,
      "moduleResolution": "node",
      "rootDir": "./webapp",
      "outDir": "./dist",
      "baseUrl": "./",
      "paths": {
        "com/ndbs/handlingunitpostui/*": [
          "./webapp/*"
        ]
      },
      "typeRoots": [
        "./node_modules/@types",
        "./node_modules/@sapui5/types",
        "./node_modules/ui5-antares"
      ]
    },
    "include": [
      "./webapp/**/*"
    ]
  }
  